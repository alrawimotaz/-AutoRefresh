{
    "manifest_version": 2,
    "name": "Reload v3",
    "version": "0.0.1",
    "description": "Auto refresh visit jordan site errors",
    "permissions": [
      "storage",
      "activeTab",
      "tabs"
    ],
    "browser_action": {
      "default_popup": "popup.html"
  
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  